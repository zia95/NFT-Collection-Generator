    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Sequence Generator File (Sample 10)",
            "type": "PowerShell",
            "request": "launch",
            "preLaunchTask": "delete seq file (sample 10)",
            "script": "./Get-NFTSequence.ps1",
            "args": ["-ConfigFile", "./config/sl_sample_10.config.json", "-OutputSequenceFile", "./config/sl_sample_10.seq.json", "-Count", "500", "-PluginModuleFilePath", "./config/sl_sample_10.plugin.psm1"]
        },
        {
            "name": "PowerShell: Merger Generator File (Final)",
            "type": "PowerShell",
            "request": "launch",
            "preLaunchTask": "delete nft file (Final)",
            "script": "./Get-NFTMerger.ps1",
            "args": ["-Multithreaded", "-ConfigFile", "./config/sl_sample_final.config.json", "-SequenceFile", "./config/sl_sample_final.seq.json", "-OutputDirectory", "./output/sl_sample_final", "-PluginModuleFilePath", "./config/sl_sample_final.plugin.psm1"]
        },
        {
            "name": "PowerShell: Merger Generator File (Final) MT OFF",
            "type": "PowerShell",
            "request": "launch",
            "preLaunchTask": "delete nft file (Final)",
            "script": "./Get-NFTMerger.ps1",
            "args": ["-ConfigFile", "./config/sl_sample_final.config.json", "-SequenceFile", "./config/sl_sample_final.seq.json", "-OutputDirectory", "./output/sl_sample_final", "-PluginModuleFilePath", "./config/sl_sample_final.plugin.psm1"]
        }
    ]
}